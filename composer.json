{
	"name": "heyday/silverstripe-fluent-inline-language",
	"description": "Allow inline language change for Accessibility in CMS WYSIWYG editor",
	"type": "silverstripe-vendormodule",
	"license": "BSD-3-Clause",
	"keywords": ["silverstripe", "WYSIWYG", "fluent", "language", "inline"],
	"homepage": "http://github.com/xini/silverstripe-fluent-inline-language",
	"authors": [{
		"name": "Florian Thoma",
		"homepage": "http://www.innoweb.com.au"
	}],
	"require": {
		"silverstripe/cms": "^4 || ^5",
		"tractorcow/silverstripe-fluent": "^4"
	},
	"suggest": {
		"innoweb/silverstripe-fluent-rtl-support": "Adds RTL support to CMS fields of sites that use fluent."
	},
	"autoload": {
		"psr-4": {
			"Innoweb\\FluentInlineLanguage\\": "src/"
		}
	},
	"extra": {
		"expose": [
			"client/dist"
		]
	}
}
